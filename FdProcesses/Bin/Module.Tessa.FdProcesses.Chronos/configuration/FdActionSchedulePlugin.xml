<?xml version="1.0" encoding="UTF-8" ?>
<plugin xmlns="http://syntellect.ru/chronos"
        launchImmediately="false"
        disallowConcurrency="true"
        disabled="false">

  <!-- Плагин по умолчанию отключён. Чтобы включить его, укажите disabled="false" -->

  <!-- Плагин запускается каждую минуту -->
  <trigger cron="25 * * * * ?" />

</plugin>
